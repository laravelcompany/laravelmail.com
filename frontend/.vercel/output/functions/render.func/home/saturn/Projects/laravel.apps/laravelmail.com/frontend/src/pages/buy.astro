---
import DefaultLayout from '../layouts/DefaultLayout.astro';
import { Image } from 'astro:assets';
import { CheckCircle, BarChart2, Mail, Shield, Users, Zap } from 'lucide-react';
// SEO Metadata
const title = "Professional Email Marketing Platform for Laravel";
const description = "Enterprise-grade email marketing platform with multi-SMTP support, advanced automation, and 800+ blade templates. Perfect for Laravel developers and businesses.";
// Features Data
const features = [
{
icon: "mail",
title: "Multi-SMTP Integration",
description: "Seamlessly integrate with multiple SMTP providers including Gmail, Amazon SES, and custom SMTP servers."
},
{
icon: "zap",
title: "Advanced Automation",
description: "Create sophisticated email workflows for onboarding, lead nurturing, and customer engagement."
},
{
icon: "chart",
title: "Header-Based Tracking",
description: "Advanced email tracking using header-based technology, providing accurate engagement metrics."
},
{
icon: "shield",
title: "Enterprise Security",
description: "Bank-grade encryption, SPF/DKIM support, and comprehensive security features."
}
];
// JSON-LD Schema
const schema = {
"@context": "https://schema.org",
"@type": "SoftwareApplication",
"name": "Laravel Email Marketing Platform",
"applicationCategory": "BusinessApplication",
"operatingSystem": "Web-based",
"offers": {
"@type": "Offer",
"price": "599.00",
"priceCurrency": "USD",
"description": "One-time payment for lifetime access"
},
"features": [
"Multi-SMTP Integration",
"Advanced Automation",
"Header-Based Tracking",
"Enterprise Security",
"800+ Laravel Blade Templates"
]
};
